{
  "format_version": "1.16.100",
  "minecraft:block": {
    "description": {
      "identifier": "pokeb:white_apricorn_sapling",
      "states": {
        "wiki:growing": [0, 1, 2]
      }
    },
    "permutations": [
      {
        "condition": "query.block_state('wiki:growing') == 0",
        "components": {
          "minecraft:on_interact": {
            "condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bone_meal')",
            "event": "bone_meal_1"
          },
          "minecraft:queued_ticking": {
            "interval_range": [120, 180],
            "on_tick": {
              "event": "grow_1"
            }
          }
        }
      },
      {
        "condition": "query.block_state('wiki:growing') == 1",
        "components": {
          "minecraft:on_interact": {
            "condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bone_meal')",
            "event": "bone_meal_2"
          },
          "minecraft:queued_ticking": {
            "interval_range": [120, 180],
            "on_tick": {
              "event": "grow_2"
            }
          }
        }
      },
      {
        "condition": "query.block_state('wiki:growing') == 2",
        "components": {
          "minecraft:on_interact": {
            "condition": "query.is_item_name_any('slot.weapon.mainhand', 0, 'minecraft:bone_meal')",
            "event": "bone_meal_3"
          },
          "minecraft:queued_ticking": {
            "interval_range": [120, 180],
            "on_tick": {
              "event": "grow_3"
            }
          }
        }
      }
    ],
    "components": {
      "minecraft:material_instances": {
        "*": {
          "texture": "white_apricorn_sapling",
          "render_method": "alpha_test",
          "face_dimming": false,
          "ambient_occlusion": false
        }
      },
      "minecraft:pick_collision": {
        "origin": [-6, 0, -6],
        "size": [12, 13, 12]
      },
      "minecraft:loot": "loot_tables/blocks/apricorn/white_apricorn_sapling.json",
      "minecraft:geometry": "geometry.apricorn_sapling",
      "minecraft:destructible_by_mining": {
        "seconds_to_destroy": 0.01
      },
      "minecraft:entity_collision": false,
      "minecraft:breathability": "air",
      "minecraft:light_dampening": 0,
      "minecraft:placement_filter": {
        "conditions": [
          {
            "block_filter": [
              "minecraft:dirt",
              "minecraft:grass",
              "minecraft:podzol"
            ],
            "allowed_faces": ["up"]
          }
        ]
      },
      "minecraft:queued_ticking": {
        "interval_range": [1, 1],
        "on_tick": {
          "event": "grow_0"
        }
      }
    },
    "events": {
      "grow_0": {
        "set_block_state": {
          "wiki:growing": 0
        }
      },
      "grow_1": {
        "sequence": [
          {
            "run_command": {
              "command": ["particle minecraft:crop_growth_emitter ~~~"]
            }
          },
          {
            "set_block_state": {
              "wiki:growing": 1
            }
          }
        ]
      },
      "grow_2": {
        "sequence": [
          {
            "run_command": {
              "command": ["particle minecraft:crop_growth_emitter ~~~"]
            }
          },
          {
            "set_block_state": {
              "wiki:growing": 2
            }
          }
        ]
      },
      "grow_3": {
        "run_command": {
          "command": [
            "particle minecraft:crop_growth_emitter ~~~",
            "scriptevent pokeb:request_tree_growth"
          ]
        }
      },
      "bone_meal_1": {
        "sequence": [
          {
            "decrement_stack": {}
          },
          {
            "trigger": {
              "event": "grow_1"
            }
          }
        ]
      },
      "bone_meal_2": {
        "sequence": [
          {
            "decrement_stack": {}
          },
          {
            "trigger": {
              "event": "grow_2"
            }
          }
        ]
      },
      "bone_meal_3": {
        "sequence": [
          {
            "decrement_stack": {}
          },
          {
            "trigger": {
              "event": "grow_3"
            }
          }
        ]
      }
    }
  }
}
